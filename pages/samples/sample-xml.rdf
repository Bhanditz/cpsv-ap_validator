<rdf:Description rdf:about="http://www.epo.org/law-practice/legal-texts/html/epc/2013/e/ar2.html">
	<rdf:type rdf:resource="http://data.europa.eu/cv/Rule"/>
	<cv:implements rdf:resource="http://www.epo.org/law-practice/legal-texts/html/epc/2013/e/ma1.html"/>
	<dct:title>Article 2 - European Patent</dct:title>
	<dct:description>Under Article 2(2) EPC, a European patent shall, in each of the contracting states for which it is granted, have the effect of and be subject to the same conditions as a national patent granted by that state, unless otherwise provided in the EPC.</dct:description>
	<dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/>
</rdf:Description>

